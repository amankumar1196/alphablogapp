<h2 class="createheading"> CREATE NEW CATEGORY </h2><br/>
</div>
<%= render 'shared/errors', obj: @category%>
        <%= form_for @category do |f| %>
            <div class="form-group row">
                <div class="control-label col-sm-2">
                    <%= f.label :Name %>
                </div>
                <div class="col-sm-8">
                <%= f.text_field :name, class:"form-control", placeholder:"Enter category name", autofocus: true %>
                </div>
            </div>
            
            <div class="form-group">
                <div class="col-md-12">
                <%= link_to 'CANCEL', categories_path, class:'btn btn-danger btn-md offset-5 mr-3'%>
                <%= f.submit class:'btn btn-success btn-md'%>
                </div>
            </div>

        <% end %>