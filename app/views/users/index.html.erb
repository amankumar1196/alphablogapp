    <h2 class="createheading"> JOURNAL BLOG USERS </h2>
</div>
<br/>

<div class="text-center pt-4">
    <% @users.each do |user| %>
        <ul class="listing">
            <div class="row ">
                <div class="col-md-2 offset-3">
                    <li><%= link_to gravatar_for(user), user_path(user) %></li>
                </div>

                <div class="col-md-2 pt-4">
                    <li class="article-title">
                        <%= link_to user.username, user_path(user) %>
                    </li>
                </div>

                <div class="col-md-2 pt-4">
                    <li><%= pluralize(user.articles.count, "Article") if user.articles %></li>
                </div>

            </div>
        </ul>
    <% end %>
</div>

<div class="offset-5 pl-4">
    <%= will_paginate %>
</div>